{
  "name": "agri-mcp",
  "version": "1.0.0",
  "description": "MCP infrastructure platform for agricultural APIs",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo build",
    "dev": "node scripts/dev.js",
    "start:local": "node scripts/start-local.js",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "check": "turbo check",
    "clean": "turbo clean",
    "test:e2e": "vitest run --config vitest.config.ts",
    "test:e2e:watch": "vitest --config vitest.config.ts",
    "supabase:start": "supabase start --workdir packages/supabase",
    "supabase:stop": "supabase stop --workdir packages/supabase",
    "supabase:reset": "supabase db reset --workdir packages/supabase"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.27.0",
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "turbo": "^2.7.3",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
